<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="it.unipi.dii.lsdb.group13.guiController.AdminMenuController"
            prefHeight="400.0" prefWidth="600.0" stylesheets="@../css/style.css"
            fx:id="menuAdmin"
>
    <top>
        <MenuBar id="menuBar" fx:id="menuBar" layoutY="-3.0" prefHeight="26.0">
            <menus>
                <Menu text = "Menu">
                    <MenuItem text="HomePage" onAction="#pressHomePage"/>
                    <MenuItem text="Cities with Most jobs" onAction="#pressCitiesAnalytic"/>
                    <MenuItem text="Most Repeated Skills" onAction="#pressSkillsAnalytic"/>
                    <MenuItem text = "Exit" onAction="#pressExit"/>
                </Menu>
            </menus>
        </MenuBar>
    </top>
</BorderPane>
